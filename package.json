{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Boilerplate for express and react",
  "main": "server.js",
  "scripts": {
    "client-install":"cd client && npm install",
    "start":"node server.js",
    "server":"nodemon server.js",
    "client":"npm start --prefix client",
    "dev":"concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Imran Ahanger",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  }
}
